<odoo>
    <data>
        <template id="assets_backend" inherit_id="web.assets_backend" name="tree view menu">
            <xpath expr="." positio="inside">
                <link rel="stylesheet" href="/helpdesk_update/static/src/css/helpdesk_update.css" />
                <script type="text/javascript" src="/helpdesk_update/static/src/js/helpdesk_update.js"></script>
            </xpath>
        </template>
        


        <template id='vista_tecnicos' name='Vista técnicos'>
            <t t-call='website.layout'>
                <div class='container p-3'>
                    <div class='row'> 
                        <div class='col-sm-12 text-center'>
                            <h3>Tickets de tecnicos</h3>
                        </div>
                    </div>
                    <div class='row'>
                        
                        <table class="table">
                            <thead class="thead-dark">
                                <tr>
                                    <th scope="col">Fecha de creación de ticket</th>
                                    <th scope="col">Número de ticket</th>
                                    <th scope="col">Etapa de ticket</th>
                                    <th scope="col">Tipo de reporte</th>
                                </tr>
                            </thead>
                            <tbody>
                                <t t-foreach='tickets' t-as='ticket'>
                                    <tr>
                                        <td>
                                            <span t-esc='ticket.create_date' />
                                        </td>
                                        <td>
                                            <span t-esc='ticket.id' />
                                        </td>
                                        <td>
                                            <span t-esc='ticket.stage_id.name' />
                                        </td>
                                        <td>
                                            <span t-esc='ticket.x_studio_tipo_de_vale' />
                                        </td>
                                    </tr>
                                </t>
                            </tbody>
                        </table>
                    </div>
                    <div class='row'> 
                        <div class='col-sm-12'>
                        </div>
                    </div>
                    <div class='row'> 
                        <div class='col-sm-12'>
                        </div>
                    </div>
                </div>
            </t>
        </template>




